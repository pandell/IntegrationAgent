  <Target Name="GetPublicKeyToken" BeforeTargets="BeforeBuild">
    <GetFrameworkSdkPath>
      <Output TaskParameter="Path" PropertyName="WindowsSdkPath" />
    </GetFrameworkSdkPath>
    <Exec Command="WHERE /r &quot;$(WindowsSdkPath.TrimEnd('\\'))&quot; sn" ConsoleToMSBuild="true">
        <Output TaskParameter="ConsoleOutput" PropertyName="SNPath" />
    </Exec>
    <PropertyGroup>
      <SNPath>$([System.Text.RegularExpressions.Regex]::Replace('$(SNPath)', ';.*', ''))</SNPath>
    </PropertyGroup>
    <Exec Command="&quot;$(SNPath)&quot; -p &quot;$(AssemblyOriginatorKeyFile)&quot; Pandell.PackageRunner.pub" />
    <Exec Command="&quot;$(SNPath)&quot; -q -t Pandell.PackageRunner.pub" ConsoleToMSBuild="true">
        <Output TaskParameter="ConsoleOutput" PropertyName="TokenFile" />
    </Exec>
    <PropertyGroup>
      <TokenFile>$([System.Text.RegularExpressions.Regex]::Replace('$(TokenFile)', 'Public key token is ', ''))</TokenFile>
    </PropertyGroup>
    <ReadLinesFromFile File="Token.cs">
      <Output TaskParameter="Lines" PropertyName="TokenCode" />
    </ReadLinesFromFile>
    <PropertyGroup>
      <TokenCode>$([System.Text.RegularExpressions.Regex]::Replace('$(TokenCode)', ';', '%0D%0A'))</TokenCode>
      <TokenCode>$([System.Text.RegularExpressions.Regex]::Replace('$(TokenCode)', '\".*\"%0D%0A', '"$(TokenFile)";'))</TokenCode>
    </PropertyGroup>
    <Message Text="$(TokenCode)" />
    <WriteLinesToFile File="Token.cs" Lines="$(TokenCode)" Overwrite="true" />
  </Target>
